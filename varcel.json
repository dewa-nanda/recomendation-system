{
    "version": 1,
    "builds": [
     {
      "src": "./bin/www.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["bin/**", "config/**", "database/**", "functions/**", "public/**", "routes/**", "validator/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "app.js"
     }
    ]
}